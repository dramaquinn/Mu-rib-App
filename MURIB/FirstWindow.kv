                root.manager.transition.direction="left"

    MDFloatLayout:
        MDCard:
            md_bg_color: 255/255,255/255,255/255,1
            size_hint:1, 1
            pos_hint:{"center_x": .5, "center_y": .4}
            radius: 150,15,15,15

#USERNAME
    MDFloatLayout:
        size_hint:0.85,0.085
        pos_hint: {"center_x": 0.5, "center_y": .75}
        MDTextField:
            id: text_field1
            hint_text: "Username"
            pos_hint:{"center_x": .5, "center_y": .58}
            icon_left: "account"
        MDLabel:
            id: username_error_label
            text: ""
            opacity: 1 if self.text else 0  # Show label only if it has text
            color: 1, 0, 0, 1  # Red
            font_style: "Caption"
            valign: "middle"
            size_hint_y: None
            height: dp(30)
            pos_hint:{"center_x": .5, "center_y": .008}

#EMAIL
    MDFloatLayout:
        size_hint:0.85,0.085
        pos_hint: {"center_x": 0.5, "center_y": .6}
        MDTextField:
            id: text_field2

            root.manager.current = 'signin'
            root.manager.transition.direction="right"

    MDFillRoundFlatButton:
        text: root.res2
        font_name: 'HS-Ishraq-Light-1.otf'
        font_size:"25sp"
        text_direction: 'rtl'
        md_bg_color: '#ba95c5'

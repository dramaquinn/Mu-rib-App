            pos_hint:{"center_x": .5, "center_y": .58}
            icon_left: "email-outline"
            helper_text: "e.g. user@gmail.com"
            helper_text_mode: "on_focus"
        MDLabel:
            id: email_error_label
            text: ""
            opacity: 1 if self.text else 0  # Show label only if it has text
            color: 1, 0, 0, 1  # Red color for error messages
            font_style: "Caption"
            valign: "middle"
            size_hint_y: None
            height: dp(30)
            pos_hint:{"center_x": 1.13, "center_y": .02}  # Adjust position as needed

#MOBILE NUMBER
    MDFloatLayout:
        size_hint: 0.85, 0.085
        pos_hint: {"center_x": 0.5, "center_y": .45}
        MDTextField:
            id: text_field3
            hint_text: "Mobile Number"
            text: "+966"
            pos_hint: {"center_x": .5, "center_y": .58}
            icon_left: "phone"
            helper_text: "e.g. 055 123 4222 "
            helper_text_mode: "on_focus"

#MOBILE NUMBER REQUIREMENT
        MDLabel:
            id: mobile_error_label
            text: ""
            color: 1, 0, 0, 1  # Red color for error messages
            font_style: "Caption"
            height: dp(30)
            pos_hint: {"center_x": .95, "center_y": .1}
           # pos: text_field2.width - self.width + dp(8), 0

#PASSWORD
    MDFloatLayout:
        size_hint:0.85,0.085
        pos_hint: {"center_x": 0.5, "center_y": .3}
        MDTextField:
            id: text_field4
            hint_text: "Password"
            pos_hint:{"center_x": .5, "center_y": .58}
            icon_left: "key-variant"
            password: True
            on_text: root.check_password_strength(self.text)  # Check password strength on text input
        MDIconButton:
            id: password_strength_icon
            icon: "eye-off"
            pos_hint: {"center_x": .95, "center_y": .53}

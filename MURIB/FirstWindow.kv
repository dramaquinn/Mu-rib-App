            id: eight_character_requirement_label
            text: "At least 8 characters"
            font_style: "Caption"
            pos_hint: {"center_x": 0.5, "center_y": 1}
            theme_text_color: "Custom"
            text_color: "#808080" # Gray color by default

        MDLabel:
            id: capital_requirement_label
            text: "At least one capital letter"
            font_style: "Caption"
            pos_hint: {"center_x": 1, "center_y": 1}
            theme_text_color: "Custom"
            text_color: "#808080" # Gray color by default

        MDLabel:

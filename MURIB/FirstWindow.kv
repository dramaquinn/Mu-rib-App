            font_style: "Caption"
            pos_hint: {"center_x": 0.5, "center_y": 0.6}
            theme_text_color: "Custom"
            text_color: "#808080" # Gray color by default

        MDLabel:
            id: small_requirement_label
            text: "At least one lowercase letter"
            font_style: "Caption"
            pos_hint: {"center_x": 1, "center_y": 0.6}
            theme_text_color: "Custom"
            text_color: "#808080" # Gray color by default


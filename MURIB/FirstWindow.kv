    MDFloatLayout:
        size_hint:0.85,0.085
        pos_hint: {"center_x": 0.5, "center_y": .2}
        MDLabel:
            id: password_strength_label
            text: ""
            color: 1, 0, 0, 1  # Red color for error messages

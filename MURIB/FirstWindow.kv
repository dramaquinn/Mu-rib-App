<FirstWindow>:
    name: 'first'
    MDFloatLayout:
        MDCard:
            md_bg_color: (1, 1, 1, 0)  # transparency
            size_hint:1, 1
            pos_hint:{"center_x": .5, "center_y": .5}
            size_hint: 0.6, 0.6  # Adjust width and height proportionally
            radius: [10]
            Image:
                source:'DohaLogo.png'
                pos_hint: {"center_x": 0.5, "center_y": .55}



    MDFillRoundFlatButton:
        text: root.res1
        font_name: 'HS-Ishraq-Light-1.otf'
        font_size:"25sp"
        text_direction: 'rtl'
        md_bg_color: '#ba95c5'
        size_hint:0.9,0.07
        pos_hint: {"center_x": 0.5, "center_y": .18}
        radius:
